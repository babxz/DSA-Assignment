#include <stdio.h>

int main() {
    int A[1][5] = {{1, 3, 4, 2, 4}};
    int B[1][5] = {{1, 2, 3, 4, 0}};
    int C[1][5]; // resulting matrix

    // perform matrix addition
    for (int i = 0; i < 1; i++) {
        for (int j = 0; j < 5; j++) {
            C[i][j] = A[i][j] + B[i][j];
        }
    }

    // print the resulting matrix
    printf("Resulting matrix C:\n");
    for (int i = 0; i < 1; i++) {
        for (int j = 0; j < 5; j++) {
            printf("%d ", C[i][j]);
        }
        printf("\n");
    }

    return 0;
}
